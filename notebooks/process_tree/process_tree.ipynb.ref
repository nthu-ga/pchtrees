   "cell_type": "code",
   "execution_count": 124,
   "id": "a2932775-af28-45c7-912f-57f945b53bfe",
   "metadata": {},
   "outputs": [],
   "source": [
    "class TreeNodeIterator():\n",
    "    def __init__(self, root):\n",
    "        self._root = root\n",
    "        self._node = self._root\n",
    " \n",
    "    def __iter__(self):\n",
    "        return self\n",
    "        \n",
    "    def __next__(self):\n",
    "        if self._node.next_dfs is None:\n",
    "            raise StopIteration\n",
    "        else:\n",
    "            this_node  = self._node\n",
    "            self._node = self._node.next_dfs    \n",
    "            return this_node\n",
    "\n",
    "    def reset(self):\n",
    "        self._node = self._root"
   ]
