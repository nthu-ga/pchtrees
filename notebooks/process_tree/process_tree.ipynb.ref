   "id": "620df3de-5085-4a71-8076-8dbe433dc463",
   "metadata": {},
   "outputs": [],
   "source": [
    "def walk_tree(node):\n",
    "    if node.next_dfs is not None:\n",
    "        return node.next_dfs\n",
    "    else:\n",
    "        return None"
   ]
